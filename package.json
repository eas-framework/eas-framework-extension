{
    "name": "beyond-easy-extension",
    "publisher": "ido-s",
    "displayName": "Beyond-easy",
    "description": "Beyond-easy Syntax highlight",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.56.0",
        "node": "14"
    },
    "scripts": {
        "build": "vsce package"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "snippets": [
			{
				"language": "html",
				"path": "./snippets/snippets.json"
			}
		],
        "grammars": [{
                "scopeName": "text.html.beyond-easy",
                "path": "./syntaxes/beyond-easy.tmLanguage.json",
                "injectTo": [
					"text.html"
				],
                "embeddedLanguages": {
					"meta.embedded.block.beyond-easy": "beyond-easy",
					"meta.embedded.block.javascript": "source.js"
				}
            }
        ],
        "configurationDefaults": {
			"emmet.includeLanguages": {
				"beyond-easy": "html"
			}
		}
    },
    "repository": {
        "url": "git@gitlab.com:ido.very.much/beyond-easy-extension.git"
    }
}